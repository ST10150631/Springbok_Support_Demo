<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Colours -->
    <!--*******************************************************************************************************************************************************************************-->


    <Color x:Key="Primary">#141e2b</Color>
    <SolidColorBrush x:Key="PrimaryBrush">#141e2b</SolidColorBrush>

    <Color x:Key="Tertiary" >#00a2ff</Color>
    <SolidColorBrush x:Key="TertiaryBrush">#00a2ff</SolidColorBrush>

    <Color x:Key="Menu">#0d6294</Color>
    <SolidColorBrush x:Key="MenuBrush">#0d6294</SolidColorBrush>



    <Color x:Key="Secondary">#10e2e6</Color>
    <SolidColorBrush x:Key="SecondaryBrush">#10e2e6</SolidColorBrush>

    <Color x:Key="Foreground">#f5f3f4</Color>
    <SolidColorBrush x:Key="ForegroundBrush">#f5f3f4</SolidColorBrush>


    <Color x:Key="RufousRed">#FF5335</Color>
    <SolidColorBrush x:Key="RufousRedBrush">#FF5335</SolidColorBrush>
    <!--*******************************************************************************************************************************************************************************-->


</ResourceDictionary>